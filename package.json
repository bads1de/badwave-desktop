{
  "name": "badwave",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "author": "Bad",
  "description": "BadWave Desktop Music Player",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "rebuild:node": "npm rebuild better-sqlite3",
    "rebuild:electron": "electron-rebuild -f -w better-sqlite3",
    "create-icons": "node scripts/create-icons.js",
    "dev:electron": "npm run build:electron && concurrently \"next dev\" \"wait-on http-get://localhost:3000 && electron .\"",
    "build:electron": "tsc -p electron",
    "package:electron": "electron-builder",
    "dist:electron": "npm run build && npm run build:electron && npm run create-icons && npm run package:electron",
    "test:electron": "node scripts/test-electron.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@react-icons/all-files": "^4.1.0",
    "@stripe/stripe-js": "^2.2.1",
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/query-async-storage-persister": "^5.90.16",
    "@tanstack/query-sync-storage-persister": "^5.90.16",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@tanstack/react-query-persist-client": "^5.90.16",
    "@tanstack/react-table": "^8.21.3",
    "@types/uniqid": "^5.3.4",
    "@use-gesture/react": "^10.3.1",
    "better-sqlite3": "^12.5.0",
    "braces": "^3.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.45.1",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "framer-motion": "^11.18.2",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.383.0",
    "music-metadata": "^7.14.0",
    "next": "^15.2.1",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "react-spinners": "^0.13.8",
    "stripe": "^14.7.0",
    "styled-components": "^6.1.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tough-cookie": "^5.0.0",
    "uniqid": "^5.4.0",
    "use-debounce": "^10.0.6",
    "user-agents": "^1.1.335",
    "ws": "^8.17.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@electron/rebuild": "^4.0.2",
    "@faker-js/faker": "^9.5.0",
    "@kayahr/jest-electron-runner": "^29.15.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__react": "^10.0.1",
    "@types/user-agents": "^1.0.4",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.31.8",
    "electron": "^34.0.0",
    "electron-builder": "^24.13.3",
    "electron-esbuild": "^9.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "png2icons": "^2.0.1",
    "postcss": "^8",
    "sharp": "^0.34.2",
    "supabase": "^1.115.4",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.badwave.app",
    "productName": "BadWave",
    "extraResources": [
      {
        "from": "public",
        "to": "public",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      "electron/**/*",
      "package.json",
      "node_modules/**/*",
      "public/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/logo.ico"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "public/logo.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "public/logo.png"
    }
  }
}
